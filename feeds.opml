<?xml version="1.0"?>
<opml version="1.0">
	<head>
		<title>RSSamantha Configuration Example</title>
	</head>
	<body>
		<outline
			title="tengtest" text="example1" description="example1description"
			config.name="example1"
			config.rsswritesleep="20543"
			config.txtwritesleep="22444"
			config.showlimit="500"
			config.storelimit="3000"
			config.rssfilename="/home/myuser/feeds/example1.rss"
			config.txtfilename="/home/myuser/feeds/example1.txt"
			config.downloadfolder="podcasts/"
			config.adddownloaditems="5"
			rsschannel.title="tengrss"
			rsschannel.link="http://myhost.example1/"
			rsschannel.description="Mostly useless Information."
			rsschannel.comment="This is the first channel with some decent sources."
			rsschannel.ttl="15"
		>
			<source title="spiegelspam" feedtype="simplerss" feedUrl="http://www.spiegel.de/spam/index.rss" delay="3000067" appenddescription="false"/>
            		<source title="wdr verkehr" feedtype="rss" feedUrl="http://www.wdr.de/verkehrslage/rss.xml" delay="60000" matchpattern_title=".*A1.*|.*A2.*" dayofweek="2,3,4,5,6" hourofday="8,9" appenddescription="true"/>
			<source title="strahotski" feedtype="rss" feedUrl="http://www.strahotski.com/feed/" delay="300000"/>
            		<source title="Niels Ruf" feedtype="rsstwitter" feedUrl="http://twitter.com/statuses/user_timeline/22873141.rss" delay="19999999"/>
			<source title="identica-dortmund" feedtype="rssidentica" feedUrl="http://identi.ca/api/statusnet/tags/timeline/dortmund.rss" delay="18300022"/>
			<source title="ubuntu users" feedtype="atom" feedUrl="http://ikhaya.ubuntuusers.de/feeds/full/20/"/>
            		<source title="oe-r" feedtype="podcast" feedUrl="http://static.orf.at/podcast/oe1/oe1_digitalleben.xml"/>
		</outline>

		<outline
			title="tengtest" text="example2text" description="example2description"
			config.name="example2"
			config.rsswritesleep="20786"
			config.showlimit="500"
			config.storelimit="3000"
			config.rssfilename="feeds/example2.rss"
			rsschannel.title="tengrss"
			rsschannel.link="http://myhost.example2/"
			rsschannel.description="Mostly useless Information."
			rsschannel.comment="This is the second channel with some decent sources."
			rsschannel.ttl="45"
		>
			<source title="Le Figaro" feedtype="rss" feedUrl="http://feeds.lefigaro.fr/c/32266/f/438191/index.rss"/>
			<!-- repl1 is given as an argument to RSSamantha and replaced at initialization time. -->
			<source title="Googlenews repl1" feedtype="rss" feedUrl="http://news.google.com/news?pz=1&amp;cf=all&amp;ned=de&amp;hl=de&amp;q=${repl1}&amp;cf=all&amp;output=rss" delay="48000000"/>
		</outline>
	</body>
</opml>



